{
  "name": "@commercelayer/cli-core",
  "version": "5.10.4",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "license": "MIT",
  "description": "Commerce Layer Javascript CLI core lib",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "exports": {
    "types": "./lib/index.d.ts",
    "require": "./lib/index.js",
    "import": "./lib/index.mjs"
  },
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lintspec": "eslint ./specs/ --ext .spec.ts",
    "lint:fix": "eslint src --fix",
    "build": "tsup",
    "start": "tsx src/index.ts",
    "test": "jest --passWithNoTests",
    "test-local": "tsx test/spot.ts",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "ecommerce",
    "jamstack",
    "commercelayer"
  ],
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@commercelayer/eslint-config-ts": "^1.4.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "oclif": "^4.22.52",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@commercelayer/js-auth": "^6.7.2",
    "@oclif/core": "^3.27.0",
    "chalk": "^4.1.2",
    "jsonwebtoken": "^9.0.3",
    "update-notifier-cjs": "^5.1.7"
  },
  "repository": "github:commercelayer/commercelayer-cli-core",
  "publishConfig": {
    "access": "public"
  }
}
